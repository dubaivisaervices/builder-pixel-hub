{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:sqlite": "node server/sqlite-server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "build:netlify": "npm run build:client && cp public/_redirects dist/spa/ && cp public/working.html dist/spa/ && cp public/emergency.html dist/spa/ && cp public/fallback.html dist/spa/ && cp public/debug.html dist/spa/ && mkdir -p netlify/functions/client/data && cp client/data/businesses.json netlify/functions/client/data/ && echo 'Business data copied successfully'",
    "start": "node dist/server/node-build.mjs",
    "start:sqlite": "node dist/server/sqlite-server.js",
    "deploy": "node deploy-simple-hostinger.js",
    "deploy-website": "node deploy-website-only.js",
    "deploy-auto": "node deploy-automatic.js --auto",
    "deploy-fix": "node deploy-database-fix.js",
    "deploy-now": "node deploy-fixed.js",
    "fix-mime": "node fix-mime-types.js",
    "fix-mime-error": "node fix-mime-error.js",
    "fix-403": "node fix-403-error.js",
    "remove-htaccess": "node remove-htaccess.js",
    "db:setup": "node database/migrate-businesses.js",
    "db:schema": "psql $NEON_DATABASE_URL -f database/schema.sql",
    "fix-react-app": "node fix-react-deployment.js",
    "diagnose-fix": "node diagnose-and-fix-deployment.js",
    "deploy-complete": "node complete-hostinger-deployment.js",
    "force-deploy": "node force-hostinger-deployment.js",
    "check-files": "node check-hostinger-files.js",
    "build-deploy-correct": "node build-and-deploy-correct.js",
    "aggressive-fix": "node aggressive-fix.js",
    "diagnose": "node diagnose-hosting-issue.js",
    "fix-deployment": "node fix-deployment-now.js",
    "create-sqlite": "node create-sqlite-database.js",
    "upload-sqlite": "node upload-sqlite-hostinger.js",
    "sqlite-full": "npm run create-sqlite && npm run upload-sqlite",
    "deploy-db": "node deploy-database.js",
    "verify": "node verify-database.js",
    "inspect-ftp": "node inspect-ftp.js",
    "explore": "node explore-ftp.js",
    "export-data": "node export-businesses.mjs",
    "export:netlify": "node quick-export.mjs",
    "quick:build": "npm run export:netlify && npm run build:client",
    "build:production": "npm run export:netlify && npm run build",
    "inspect-schema": "node inspect-schema.mjs",
    "test-ftp": "node test-ftp.js",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "check.types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.845.0",
    "@aws-sdk/s3-request-presigner": "^3.845.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-query": "^5.62.10",
    "@tanstack/react-router": "^1.89.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "axios": "^1.10.0",
    "basic-ftp": "^5.0.5",
    "better-sqlite3": "^11.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lucide-react": "^0.468.0",
    "multer": "^2.0.1",
    "mysql2": "^3.11.6",
    "pg": "^8.12.0",
    "next-themes": "^0.4.4",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3",
    "serverless-http": "^3.2.0",
    "sonner": "^1.7.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.89.0",
    "@tanstack/router-plugin": "^1.89.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^2.1.8"
  }
}
